{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "web",
      "host": {
        "sourcePath": "/home/ec2-user/ccg-app"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "beeis/woodcard-nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        },
        {
          "hostPort": 443,
          "containerPort": 443
        }
      ],
      "links": [
        "php"
      ],
      "volumesFrom": [
        {
          "sourceContainer": "web"
        }
      ]
    },
    {
      "name": "php",
      "image": "beeis/php",
      "essential": true,
      "memory": 512,
      "volumesFrom": [
        {
          "sourceContainer": "web"
        }
      ]
    },
    {
      "name": "web",
      "image": "beeis/woodcard-app",
      "essential": true,
      "memory": 128
    }
  ]
}
